{
	"@context": "http://www.w3.org/ns/csvw",
	"url": "../input/artworks.csv",
	"dialect": {
		"delimiter": ",",
		"doubleQuote": true,
		"encoding": "utf-8",
		"header": true,
		"headerRowCount": "1",
		"lineTerminators": "\r\n",
		"quoteChar": "\"",
		"trim": false
	},
	"tableSchema": {
