				"valueUrl": "https://pe.plateforme10.ch/Artwork/{arkID}/Title#{titreType_string}"
			},
			{
				"propertyUrl": "http://www.cidoc-crm.org/cidoc-crm/P108i_was_produced_by",
				"titles": "arkID",
				"valueUrl": "https://pe.plateforme10.ch/Event/Production/{arkID}-{event_prod_start}/identifier"
