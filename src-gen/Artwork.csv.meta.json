			},
			{
				"propertyUrl": "http://www.cidoc-crm.org/cidoc-crm/P1_is_identified_by",
				"titles": "arkID",
				"valueUrl": "https://pe.plateforme10.ch/Artwork/{arkID}/identifier"
			},
			{
				"propertyUrl": "http://www.cidoc-crm.org/cidoc-crm/P1_is_identified_by",
