				"titles": "WikipediaName"
			},
			{
				"suppressOutput": true,
				"titles": "institutionName_fr"
			},
			{
				"suppressOutput": true,
				"titles": "addrStreet"
			},
			{
				"suppressOutput": true,
				"titles": "addrZIP"
			},
			{
				"suppressOutput": true,
				"titles": "addrCity"
			},
			{
				"suppressOutput": true,
				"titles": "addrCountry_fr"
			}
		]
	}
