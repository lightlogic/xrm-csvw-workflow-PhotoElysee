dialect CommaPE {
    delimiter ","
    doubleQuote true
    encoding "utf-8"
    header true
	headerRowCount 1
	lineTerminators "\\r\\n"
	quoteChar '\\"'
	trim false
}

logical-source Creators {
    type csv
    source "../input/creators.csv"
    dialect CommaPE

    referenceables
        creator_UK
        primaryName_fr
        givenName
        LastName
        birthName
        creatorQ
        birthDate
        deathDate
}