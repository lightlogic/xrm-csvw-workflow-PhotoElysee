// ** Artwork Title ** //
  // following Linked Art guidelines
  // BASELINE PATTERNS, TYPES: https://linked.art/model/base/#types-of-types

output csvw

template art_TypeIri "https://pe.plateforme10.ch/Artwork/{0}/Type" 

map Type from Artworks {
	subject template art_TypeIri with arkID;

	types
		crm.E55_Type

	properties
		rdfs.label from art_typeAAT_label;	// type label for documentation purpose
		crm.P2_has_type constant "http://vocab.getty.edu/aat/300133025"; // [type] works of art
}